{
  "port": 9000,
  "host": "0.0.0.0",
  "logging": {
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "standard": {
        "format": "%(asctime)s %(levelname)s: %(message)s"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "formatter": "standard",
        "level": "INFO",
        "stream": "ext://sys.stdout"
      },
      "file": {
        "class": "logging.handlers.RotatingFileHandler",
        "formatter": "standard",
        "filename": "car.log",
        "maxBytes": 1024,
        "backupCount": 3
      }
    },
    "loggers": {
      "": {
        "handlers": ["console", "file"],
        "level": "DEBUG",
        "propagate": false
      }
    }
  }
}
